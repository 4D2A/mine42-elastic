input {
  rabbitmq {
    id => "input.rabbitmq"
    host => "${RABBITMQ_HOST}"
    exchange => "mine42.logs"
    exchange_type => "topic"
    durable => true
    add_field => { "[@metadata][index_prefix]" => "alerts.lux.xlm.soc" }
  }
}


output {
  elasticsearch {
    id => "output.elasticsearch"
    hosts => "${ELASTICSEARCH_HOSTS}"
  }
}